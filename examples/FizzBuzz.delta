import "stdio.h"

func fizzbuzz(limit: int) {
  for (i in 1...limit) {
    if (i % 15 == 0) {
      printf("fizzbuzz\n")
    } else if (i % 3 == 0) {
      printf("fizz\n")
    } else if (i % 5 == 0) {
      printf("buzz\n")
    } else {
      printf("%d\n", i)
    }
  }
}

func main() {
  fizzbuzz(15)
}
